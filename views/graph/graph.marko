    <script>
      anychart.onDocumentLoad(function () {
        // create an instance of a pie chart
      
      var $box = $("input#data");
      var data = $box.val();
      var $box2 = $("input#string");
      var string = $box2.val();
      var $box3 = $("input#sar");
      var sar = $box3.val();
      var $box4 = $("input#entrada");
      var entrada = $box4.val();
      var $box5 = $("input#pontosA");
      var pontos = $box5.val();
      var $box6 = $("input#random1");
      var random1 = $box6.val();
      var $box7 = $("input#random2");
      var random2 = $box7.val();
      var $box8 = $("input#movimentoA");
      var movimentoA = $box8.val();
      var $box9 = $("input#correcaoA");
      var correcaoA = $box9.val();
      var $box10 = $("input#movimento");
      var movimento = $box10.val();
      var $box11 = $("input#correcao");
      var correcao = $box11.val();
      var $box12 = $("input#movimentoAS");
      var movimentoAS = $box12.val();
      var $box13 = $("input#correcaoAS");
      var correcaoAS = $box13.val();
      var $box14 = $("input#movimentoS");
      var movimentoS = $box14.val();
      var $box15 = $("input#correcaoS");
      var correcaoS = $box15.val();
      string = string.split(',');
      data = data.split(',');
      sar = sar.split(',');
      entrada = entrada.split(',');
      pontos = pontos.split(',');
      random1 = random1.split(',');
      random2 = random2.split(',');
      movimentoA = movimentoA.split(',');
      correcaoA = correcaoA.split(',');
      movimento = movimento.split(',');
      correcao = correcao.split(',');
      movimentoAS = movimentoAS.split(',');
      correcaoAS = correcaoAS.split(',');
      movimentoS = movimentoS.split(',');
      correcaoS = correcaoS.split(',');
      var dados = [];
      var array = [];
      for(var x = 0; x < data.length ; x++){
        array = [string[x],data[x]];
        dados.push(array);
      }
      
      var chart = anychart.column();
      chart.title("Pks - LogNormal");
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      var series = chart.column(dados);
      chart.container("container1");
      chart.draw();
      
      string = ["P1","P2","P3","P2novo", "P3novo"];
      array1 = [];
      var dados1 = [];
      array = [];
      dados = []; 
      for(var x = 0; x < entrada.length ; x++){
        array = [`valor ${x}`, entrada[x]];
        array1 = [`valor ${x}`, sar[x]];
          //array1 = array;
        dados.push(array);
        dados1.push(array1);
      }
      stage = anychart.graphics.create("container2");

      // create a chart
      var chart = anychart.line();

      dataSet = (dados);
      dataSet1 = (dados1);
      
      series = chart.line(dataSet);
      series.name("Valor Real")
      series1 = chart.line(dataSet1);
      series1.stroke("green");
      series1.name("SAR");
      var scale = chart.yScale();
      // Sets scale maximum.
      //scale.maximum(2);

      // add a title standalone object on the chart
      chart.title("Dados e SAR");
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");

      // put the chart on a stage and draw it
      chart.container(stage);
      chart.draw();
      
      dados = [];
      array = [];
      for(var x = 0; x < pontos.length ; x++){
        array = [`ponto ${x}`, pontos[x]];
        dados.push(array);
      }
      
      var chart = anychart.line();
      chart.title("Pontos Artificiais");
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      var series = chart.line(dados);
      series.name("Ponto artificial");
      chart.container("container3");
      chart.draw();

      array1 = [];
      dados1 = [];
      array = [];
      dados = []; 
      for(var x = 0; x < random1.length ; x++){
        array = [`${x}`, random1[x]];
        array1 = [`${x}`, random2[x]];
          //array1 = array;
        dados.push(array);
        dados1.push(array1);
      }
      stage = anychart.graphics.create("container4");

      // create a chart
      chart = anychart.line();
      chart.xScale(anychart.scales.linear());

      dataSet = (dados);
      dataSet1 = (dados1);
      
      series = chart.line(dataSet);
      series.name("Movimento Artificial")
      series1 = chart.line(dataSet1);
      series1.stroke("green");
      series1.name("Correção Artificial");
      var scale = chart.yScale();
      // Sets scale maximum.
      //scale.maximum(2);

      // add a title standalone object on the chart
      chart.title("Acumulados Randômicos: Movimento Artificial & Correção Artificial");
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");

      // put the chart on a stage and draw it
      chart.container(stage);
      chart.draw();

      dados = [];
      array = [];
      for(var x = 0; x < movimentoA.length ; x++){
        array = [`${x}`,movimentoA[x]];
        dados.push(array);
      }
      
      chart = anychart.column();
      chart.title("Movimento Artificial");
      var xScale = chart.xScale();
      console.log(movimentoAS);
      console.log(movimentoAS[2]);
      xScale.minimum(Number(movimentoAS[0]));
      xScale.maximum(Number(movimentoAS[1]));
      xScale.ticks().interval(Number(movimentoAS[2]));
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      series = chart.column(dados);
      chart.container("container7");
      chart.draw();

      dados = [];
      array = [];
      for(var x = 0; x < correcaoA.length ; x++){
        array = [`${x}`,correcaoA[x]];
        dados.push(array);
      }
      
      chart = anychart.column();
      chart.title("Correção Artificial");
      var xScale = chart.xScale();
      xScale.minimum(Number(correcaoAS[0]));
      xScale.maximum(Number(correcaoAS[1]));
      xScale.ticks().interval(Number(correcaoAS[2]));
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      series = chart.column(dados);
      chart.container("container8");
      chart.draw();

      dados = [];
      array = [];
      for(var x = 0; x < movimento.length ; x++){
        array = [`${x}`,movimento[x]];
        dados.push(array);
      }
      
      chart = anychart.column();
      chart.title("Movimento Real");
      var xScale = chart.xScale();
      xScale.minimum(Number(movimentoS[0]));
      xScale.maximum(Number(movimentoS[1]));
      xScale.ticks().interval(Number(movimentoS[2]));
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      series = chart.column(dados);
      chart.container("container5");
      chart.draw();

      dados = [];
      array = [];
      for(var x = 0; x < correcao.length ; x++){
        array = [`${x}`,correcao[x]];
        dados.push(array);
      }
      
      chart = anychart.column();
      chart.title("Correção Real");
      var xScale = chart.xScale();
      xScale.minimum(Number(correcaoS[0]));
      xScale.maximum(Number(correcaoS[1]));
      xScale.ticks().interval(Number(correcaoS[2]));
      chart.dataArea().background().enabled(true);
      chart.dataArea().background().fill("#665B59 0.2");
      series = chart.column(dados);
      chart.container("container6");
      chart.draw();
      

    });
  </script>
